<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/ns_raes/db" VERSION="20160126" COMMENT="XMLDB file for Moodle blocks/ns_raes"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_ns_raes" COMMENT="Default comment for ns_raes, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="blockid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Block id"/>
        <FIELD NAME="pagetitle" TYPE="char" LENGTH="30" NOTNULL="true" SEQUENCE="false" COMMENT="Link Title"/>
        <FIELD NAME="linkdescription" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="A description for your link"/>
        <FIELD NAME="linkurl" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false" COMMENT="Url"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="component" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="ns_raes_editing_request" COMMENT="Default comment for the table, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="blockid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="pagetitle" TYPE="char" LENGTH="30" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="linkdescription" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="linkurl" TYPE="char" LENGTH="1333" NOTNULL="true" SEQUENCE="false" COMMENT="Url"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="component" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="raeid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Rae record to be modified, new record if null"/>
        <FIELD NAME="authorid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="modification author"/>
        <FIELD NAME="request_type" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" COMMENT="0-Edit 1-Delete 2-Insert"/>
        <FIELD NAME="state" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="0-Requested 1-Approved 2-Rejected"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>