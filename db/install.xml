        <FIELD NAME="item_id" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="itemid -&amp;gt; File table"/>
        <FIELD NAME="click_count" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="Number of clicks on each record"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>